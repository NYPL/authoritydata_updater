    <analyzer>
      <tokenizer class="solr.StandardTokenizerFactory"/>
      <filter class="solr.TurkishLowerCaseFilterFactory"/>
      <filter class="solr.StopFilterFactory" words="lang/stopwords_tr.txt" ignoreCase="false"/>
      <filter class="solr.SnowballPorterFilterFactory" language="Turkish"/>
    </analyzer>
  </fieldType>
  <fieldType name="text_ws" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <tokenizer class="solr.WhitespaceTokenizerFactory"/>
    </analyzer>
  </fieldType>
  <field name="_nest_path_" type="_nest_path_"/>
  <field name="_root_" type="string" docValues="false" indexed="true" stored="false"/>
  <field name="_text_" type="text_general" multiValued="true" indexed="true" stored="false"/>
  <field name="_version_" type="plong" indexed="false" stored="false"/>
  <field name="alternate_term" type="text_general"/>
  <field name="alternate_term_idx" type="text_general"/>
  <field name="authority_code" type="text_general"/>
  <field name="authority_name" type="text_general"/>
  <field name="biog_note" type="text_general"/>
  <field name="birth_year" type="plongs"/>
  <field name="broader_term" type="text_general"/>
  <field name="broader_term_idx" type="text_general"/>
  <field name="death_year" type="plongs"/>
  <field name="language" type="text_general"/>
  <field name="record_id" type="text_general"/>
  <field name="term" type="smart_text"/>
  <field name="term_idx" type="smart_text"/>
  <field name="term_type" type="smart_text"/>
  <field name="unique_id" type="string"/>
  <field name="uri" type="text_general"/>
  <copyField source="alternate_term" dest="alternate_term_idx"/>
  <copyField source="broader_term" dest="broader_term_idx"/>
  <copyField source="term" dest="term_idx"/>
</schema>